    "express-validator": "^7.0.1",
    "http-status-codes": "^2.3.0",
    "morgan": "^1.10.0",
    "pg": "^8.13.0",
    "sequelize": "^6.37.3",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.41",
