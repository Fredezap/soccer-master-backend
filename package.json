{
  "name": "soccer-master-backend",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "swc build ./src -d ./dist --source-maps",
    "serve": "NODE_ENV=production node ./dist/app.js",
